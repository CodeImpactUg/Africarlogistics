{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "plugin:prettier/recommended"
    ],
    "parseOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "warn",
        "no-eval": "error",
        "prettier/prettier": [
            "error",
            {
                "singleQuote": true
            }
        ]
    }
}